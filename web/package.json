{
    "name": "metamist",
    "version": "7.10.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.11.5",
        "@duckdb/duckdb-wasm": "^1.29.0",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@monaco-editor/react": "^4.6.0",
        "@mui/icons-material": "^6.1.6",
        "@mui/material": "^6.1.6",
        "@mui/x-data-grid": "^7.22.2",
        "@observablehq/plot": "^0.6.16",
        "arquero": "^7.2.0",
        "axios": "^0.24.0",
        "common-tags": "^1.8.2",
        "d3": "^7.9.0",
        "d3-scale-chromatic": "^3.1.0",
        "graphql": "^16.6.0",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "luxon": "^3.5.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-google-slides": "^4.0.0",
        "react-icons": "^5.2.1",
        "react-markdown": "^7.1.0",
        "react-responsive": "^9.0.2",
        "react-router-dom": "^6.0.1",
        "react-syntax-highlighter": "^15.4.4",
        "react-use": "^17.5.1",
        "react-virtuoso": "^4.6.3",
        "remark-gfm": "^3.0.1",
        "remark-toc": "^8.0.1",
        "semantic-ui-css": "^2.5.0",
        "semantic-ui-react": "^2.1.3",
        "swagger-ui-react": "^4.13.0",
        "web-vitals": "^1.1.2"
    },
    "proxy": "http://localhost:8000",
    "type": "module",
    "scripts": {
        "start": "vite --config vite.config.js",
        "prebuild": "npm run compile",
        "typecheck": "tsc",
        "build": "tsc && vite build --config vite.config.js",
        "preview": "vite preview --config vite.config.js",
        "postbuild": "npm run copy:buildartifacts",
        "copy:buildartifacts": "rm -rf ../api/public/ && mkdir -p ../api/public && cp -r dist/* ../api/public/",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint --fix . --ext .ts,.tsx",
        "format": "prettier --write 'src/**/*.{ts,tsx,css}' --config ./.prettierrc.yaml",
        "format:check": "prettier --check 'src/**/*.{ts,tsx,css}' --config ./.prettierrc.yaml",
        "compile": "cd .. && strawberry export-schema api.graphql.schema:schema > web/schema.graphql && cd - && graphql-codegen",
        "watch": "graphql-codegen -w"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "@graphql-codegen/cli": "^3.3.1",
        "@graphql-codegen/client-preset": "^2.1.0",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "@types/common-tags": "^1.8.4",
        "@types/d3": "^7.4.3",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.178",
        "@types/luxon": "^3.4.2",
        "@types/node": "^18.11.18",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-grid-layout": "^1.3.2",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-syntax-highlighter": "^13.5.2",
        "@types/swagger-ui-react": "^3.35.3",
        "@typescript-eslint/eslint-plugin": "^8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.56.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.5.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "prettier": "^3.3.0",
        "prettier-plugin-organize-imports": "^3.2.4",
        "react-error-overlay": "^6.0.9",
        "ts-node": "^10.9.1",
        "typescript": "^5.5.4",
        "vite": "^5.2.10",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-svgr": "^4.2.0"
    }
}
