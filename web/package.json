{
    "name": "metamist",
    "version": "6.10.3",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.7.3",
        "@artsy/fresnel": "^6.2.1",
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@mui/icons-material": "^5.10.9",
        "@mui/material": "^5.10.9",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.24",
        "@types/node": "^16.11.7",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.3",
        "axios": "^0.24.0",
        "d3": "^7.6.1",
        "graphql": "^16.6.0",
        "lodash": "^4.17.21",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-grid-layout": "^1.3.4",
        "react-intersection-observer": "^9.5.2",
        "react-markdown": "^7.1.0",
        "react-responsive": "^9.0.2",
        "react-router-dom": "^6.0.1",
        "react-syntax-highlighter": "^15.4.4",
        "react-virtuoso": "^4.6.3",
        "recharts": "^2.8.0",
        "remark-gfm": "^3.0.1",
        "remark-toc": "^8.0.1",
        "semantic-ui-css": "^2.5.0",
        "semantic-ui-react": "^2.1.3",
        "swagger-ui-react": "^4.13.0",
        "vite-plugin-svgr": "^2.4.0",
        "web-vitals": "^1.1.2"
    },
    "proxy": "http://localhost:8000",
    "type": "module",
    "scripts": {
        "start": "vite --config vite.config.js",
        "prebuild": "npm run compile",
        "build": "vite build --config vite.config.js",
        "preview": "vite preview --config vite.config.js",
        "postbuild": "npm run copy:buildartifacts",
        "copy:buildartifacts": "rm -rf ../api/public/ && mkdir -p ../api/public && cp -r dist/* ../api/public/",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint --fix . --ext .ts,.tsx",
        "format": "prettier --write src/**/*.{ts,tsx,css} --config ./.prettierrc",
        "compile": "cd .. && strawberry export-schema api.graphql.schema:schema > web/schema.graphql && cd - && graphql-codegen",
        "watch": "graphql-codegen -w"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "@graphql-codegen/cli": "^3.2.1",
        "@graphql-codegen/client-preset": "^2.1.0",
        "@types/d3": "^7.4.0",
        "@types/lodash": "^4.14.178",
        "@types/node": "^18.11.18",
        "@types/react-grid-layout": "^1.3.2",
        "@types/react-syntax-highlighter": "^13.5.2",
        "@types/swagger-ui-react": "^3.35.3",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "@vitejs/plugin-react": "^4.0.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^2.8.2",
        "react-error-overlay": "^6.0.9",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "vite": "^4.3.3",
        "vite-plugin-html": "^3.2.0"
    }
}
