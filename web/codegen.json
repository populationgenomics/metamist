{
    "schema": "schema.graphql",
    "documents": "src/**/*.{tsx,ts,js,jsx}",
    "emitLegacyCommonJSImports": false,
    "generates": {
        "./src/__generated__/": {
            "preset": "client",
            "presetConfig": {
                "gqlTagName": "gql",
                "fragmentMasking": false

            }
        }
    },
    "ignoreNoDocuments": false,
    "noSilentErrors": true
}
